{
	"name": "my-svelte-boilerplate-v10",
	"type": "module",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"lint": "eslint --fix --config eslint.config.js . & stylelint --fix --ignore-path .gitignore --config .stylelintrc.yaml ./**/*.{css,svelte}",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"prepare": "husky",
		"preinstall": "npx -y only-allow bun"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^2.27.1",
		"@iodigital/vite-plugin-msw": "^2.0.0",
		"@sveltejs/adapter-auto": "^3.2.4",
		"@sveltejs/kit": "^2.5.24",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.9.1",
		"eslint-plugin-svelte": "2.36.0-next.13",
		"husky": "^9.1.5",
		"lint-staged": "^15.2.9",
		"msw": "^2.3.5",
		"postcss": "^8.4.41",
		"postcss-load-config": "^5.1.0",
		"stylelint": "^16.8.2",
		"stylelint-config-html": "^1.1.0",
		"stylelint-config-recess-order": "^5.1.0",
		"stylelint-config-standard": "^36.0.1",
		"stylelint-declaration-block-no-ignored-properties": "^2.8.0",
		"svelte": "^4.2.19",
		"svelte-check": "^3.8.6",
		"svelte-eslint-parser": "^0.34.1",
		"tailwindcss": "^3.4.10",
		"tslib": "^2.7.0",
		"typescript": "^5.5.4",
		"vite": "^5.4.2"
	}
}
